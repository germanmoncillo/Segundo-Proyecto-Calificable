    <header>
        <h1>Centro Medico Stanford</h1>
    </header>
    <main>
        <h2>Listado medicos</h2><br>
        <img src="/images/escudo.png" width="80px">
        <table id="tablaDatos" border="2">
            <thead>
                <tr>
                <th>Fecha cita</th>
                <th>Paciente</th>
                <th>Telefono paciente</th>
                <th>Especialidad</th>
                <th>Consultorio</th>
                <th>Medico</th>
                </tr>
            </thead>
            <tbody>
            {{#each resultado}}
            <tr>
                <td>{{formatoFecha fecha_cita}}</td>
                <td>{{nombres}} {{apellidos}}</td>
                <td>{{telefono}}</td>
                <td>{{especialidad}}</td>
                <td>{{consultorio}}</td>
                <td>{{nombresMedico}} {{apellidosMedico}}</td>
            </tr>
            {{/each}}
            </tbody>
        </table>
        <a href="/index.html"><input type="button" value="Volver"></a>
    </main>
    <div class="capa"></div>
    <footer>
        <h2 class="titulo-final">&copy; GmC Desing | German Moreno</h2>
        <audio autoplay controls>
            <source src="/images/leva-eternity-149473.mp3" type="audio/mp3">
        </audio>
    </footer>