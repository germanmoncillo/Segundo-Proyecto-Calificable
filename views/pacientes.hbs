<header>
        <h1>Centro Medico Stanford</h1>
    </header>
    <main>
        <h2>Listado pacientes</h2> <br>
        <img src="/images/escudo.png" width="80px">
        <table id="contenidoPacientes" border="2">
            <thead>
                <tr>
                    <th>Nombres</th>
                    <th>Apellidos</th>
                    <th>Número de Cédula</th>
                    <th>Fecha de nacimiento</th>
                    <th>Telefono</th>
                </tr>
            </thead>
            <tbody>
                {{#each resultado}}
                <tr>
                    <td>{{nombres}}</td>
                    <td>{{apellidos}}</td>
                    <td>{{cedula}}</td>
                    <td>{{formatoFecha fecha_nacimiento}}</td>
                    <td>{{telefono}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
            <a href="/index.html"><input type="button" value="Volver"></a>
    </main>
    <div class="capa"></div>
        <footer>
            <h2 class="titulo-final">&copy; GmC Desing | German Moreno</h2>
            <audio autoplay controls>
                <source src="/images/leva-eternity-149473.mp3" type="audio/mp3">
            </audio>
        </footer>