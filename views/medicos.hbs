    <header>
        <h1>Centro Medico Stanford</h1>
    </header>
    <main>
        <h2>Listado medicos</h2><br>
        <img src="/images/escudo.png" width="80px">
        <table id="tablaDatos" border="2">
            <thead>
                <tr>
                    <th>Nombres</th>
                    <th>Apellidos</th>
                    <th>Número de Cédula</th>
                    <th>Consultorio</th>
                    <th>Telefono</th>
                    <th>Correo</th>
                    <th>Especialidad</th>
                </tr>
            </thead>
            <tbody>
            {{#each resultado}}
                <tr>
                <td>{{nombres}}</td>
                <td>{{apellidos}}</td>
                <td>{{cedula}}</td>
                <td>{{consultorio}}</td>
                <td>{{telefono}}</td>
                <td>{{correo}}</td>
                <td>{{especialidad}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
        <a href="/index.html"><input type="button" value="Volver"></a>
    </main>
    <div class="capa"></div>
    <footer>
        <h2 class="titulo-final">&copy; GmC Desing | German Moreno</h2>
        <audio autoplay controls>
            <source src="/images/leva-eternity-149473.mp3" type="audio/mp3">
        </audio>
    </footer>